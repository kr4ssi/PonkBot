  "bugs": {
    "url": "https://github.com/Xaekai/PonkBot/issues"
  },
  "homepage": "https://github.com/Xaekai/PonkBot#readme",
  "dependencies": {
    "chalk": "github:chalk/chalk",
    "cheerio": "1.0.0-rc.3",
    "cloudscraper": "4.1.2",
    "date-and-time": "latest",
    "dotenv": "latest",
    "express": "latest",
    "forwarded": "latest",
    "gitlab": "latest",
    "i18n-iso-countries": "latest",
    "knex": "latest",
    "limiter": "latest",
    "mysql": "latest",
    "python-shell": "1.0.7",
