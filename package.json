  "main": "service.js",
  "scripts": {
    "test": "echo \"Notice: Tests coming soon.\" && exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Xaekai/PonkBot.git"
  },
  "keywords": [
    "cytube",
    "bot",
    "ponkbot"
  ],
